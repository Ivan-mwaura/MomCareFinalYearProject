{
    "builds": [
      {
        "src": "model.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/predict",
        "methods": ["POST"],
        "dest": "model.py"
      },
      {
        "src": "/debug",
        "methods": ["GET"],
        "dest": "model.py"
      },
      {
        "src": "/",
        "methods": ["GET"],
        "dest": "model.py"
      }
    ]
  }